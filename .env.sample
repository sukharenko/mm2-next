# Google Maps API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Home Location (latitude:longitude)
# This is where your ADS-B receiver is located
NEXT_PUBLIC_LOCATION=27.2579:33.8116

# dump1090 Configuration
# Host where dump1090 is running
DUMP1090_HOST=192.168.3.7
# Port for dump1090 JSON API (usually 8080)
DUMP1090_JSON_PORT=8080

# FlightAware AeroAPI v4
# Get your API key from: https://www.flightaware.com/commercial/aeroapi/
FLIGHTAWARE_API_KEY=your_flightaware_api_key_here
